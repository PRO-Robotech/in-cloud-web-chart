kubectl get clusterclaimnew apmistcs-test-19 -o json \
| jq -r '
  .items[]
  | {
      name: .metadata.name,
      created: (.metadata.creationTimestamp | sub(":[0-9][0-9]Z$"; ":00Z")),
      ready: (.status.conditions[] | select(.type=="Ready") | .lastTransitionTime)
    }
  | .delta = ((.ready | fromdateiso8601) - (.created | fromdateiso8601))
  | "\(.name)  \(.delta)s"
'